{
	"manifest_version": 2,
	"name": "GoloAds",
	"description": "Описание расширения",
	"version": "1.0.0",
	"icons": {
		"128": "./images/icon.png"
	},
	"browser_action": {
		"default_icon": "./images/icon.png",
		"default_popup": "popup.html"
	},
	"permissions": [
		"activeTab"
	],
	"content_scripts" : [{
		"css" : ["./css/content_style.css"],
		"matches" : ["https://www.google.com/*"]
	}]
}